@model IEnumerable<Docker.Benchmarking.Orchestrator.Web.ViewModels.BenchmarkTestItemViewModel>


<div class="table-responsive">
    <table id="web-server-benchmark-table" class="table table-striped table-sm" style="width:100%">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Timestamp)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Elapsed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Label)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ResponseCode)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.ResponseMessage)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.ThreadName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Success)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FailureMessage)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Bytes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SentBytes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GroupThreads)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AllThreads)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Latency)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IdleTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Connect)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Timestamp)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Elapsed)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Label)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ResponseCode)
                    </td>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.ResponseMessage)
                        </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.ThreadName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DataType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Success)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FailureMessage)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Bytes)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SentBytes)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.GroupThreads)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AllThreads)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Latency)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdleTime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Connect)
                    </td>
                </tr>
            }
        </tbody>
        <tfoot>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Timestamp)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Elapsed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Label)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ResponseCode)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.ResponseMessage)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.ThreadName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Success)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FailureMessage)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Bytes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SentBytes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GroupThreads)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AllThreads)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Latency)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IdleTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Connect)
                </th>
            </tr>
        </tfoot>

    </table>
</div>
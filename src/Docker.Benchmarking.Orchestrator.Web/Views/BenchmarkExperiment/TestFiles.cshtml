
@{
    ViewData["Title"] = "Apache Jmeter Test Files";
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
    <h1 class="h2">@ViewData["Title"]</h1>
</div>

<div class="row">
    <div class="col-md-12">
        @await Component.InvokeAsync("ApacheJmeterTestFilesTable")
    </div>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $('#jmeter-files-table').DataTable();
        });
    </script>
}

